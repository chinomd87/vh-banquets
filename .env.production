# Production Environment Variables for vhbanquets.com
REACT_APP_ENV=production
REACT_APP_API_URL=https://api.vhbanquets.com
REACT_APP_DOMAIN=vhbanquets.com
REACT_APP_SITE_NAME=VH Banquets
REACT_APP_VERSION=$npm_package_version

# Security
REACT_APP_ENABLE_DEVTOOLS=false
GENERATE_SOURCEMAP=false

# Performance
INLINE_RUNTIME_CHUNK=false
IMAGE_INLINE_SIZE_LIMIT=8192

# Analytics (add your tracking IDs)
# REACT_APP_GA_TRACKING_ID=G-XXXXXXXXXX
# REACT_APP_HOTJAR_ID=XXXXXXX

# API Configuration
REACT_APP_API_TIMEOUT=10000
REACT_APP_MAX_FILE_SIZE=10485760

# RhodeSign Integration
REACT_APP_RHODESIGN_URL=https://rhodesign.app
REACT_APP_RHODESIGN_ENABLED=true

# Feature Flags
REACT_APP_ENABLE_PDF_ANALYSIS=true
REACT_APP_ENABLE_FLOOR_PLAN_EDITOR=true
REACT_APP_ENABLE_STAFF_MANAGEMENT=true
REACT_APP_ENABLE_RHODESIGN_INTEGRATION=true
